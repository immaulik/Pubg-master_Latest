<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pubgtournament">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.ReferAndEarn"></activity>
        <activity
            android:name=".Activities.PopUpBalanceActivity"
            android:theme="@style/AppTheme.PopMe" />
        <activity android:name=".Activities.ForgotPasswordActivity" />
        <activity android:name=".Activities.ResultContests" />
        <activity android:name=".Activities.AddAmount" />
        <activity
            android:name=".Activities.UpcomingContests"
            android:parentActivityName=".Activities.Joined_Contests" />
        <activity
            android:name=".Activities.Joined_Contests"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.SquadMatchInfo"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.DuoMatchInfo"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.SoloMatchInfo"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.ProfileActivity"
            android:parentActivityName=".Activities.MainActivity" />
        <activity android:name=".Activities.LoginUser" />
        <activity android:name=".Activities.RegisterUser" />
        <activity android:name=".Activities.MainActivity" />
        <activity
            android:name=".Activities.WalletActivity"
            android:parentActivityName=".Activities.MainActivity" />
        <activity
            android:name=".Activities.splashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>